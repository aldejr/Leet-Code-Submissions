      "source": [
        "def addBinary(a,b):\n",
        "  #Convert inputs to base-2 values\n",
        "  addend1 = int(a,2)\n",
        "  addend2 = int(b,2)\n",
        "  #return string of their sum\n",
        "  return bin(addend1+addend2)[2:]"
      ]
    },
