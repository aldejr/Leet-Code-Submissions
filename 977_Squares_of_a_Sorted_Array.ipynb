      "source": [
        "def sortedSquares(nums):\n",
        "  #Initialise Output\n",
        "  out = []\n",
        "  #Add squared values to output\n",
        "  for num in nums:\n",
        "    out.append(num**2)\n",
        "  #Sort the output\n",
        "  return sorted(out)\n"
      ]
    },
